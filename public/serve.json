{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=63072000; includeSubDomains; preload"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/(.*)/?",
      "destination": "https://partysl-dev.herokuapp.com/$1",
      "type": 301
    }
  ]
}
